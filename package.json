  "homepage": "https://github.com/julien-f/simple-front-http-server",
  "bugs": "https://github.com/julien-f/simple-front-http-server/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-f/simple-front-http-server"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "bin": {
    "simple-front-http-server": "dist/cli"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "app-conf": "^0.4.0",
    "babel-runtime": "^6.11.6",
    "event-to-promise": "^0.7.0",
    "exec-promise": "^0.6.1",
    "fs-promise": "^0.5.0",
    "http-proxy": "^1.14.0",
    "http-server-plus": "^0.7.0",
    "koa": "^2.0.0",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^2.0.0",
    "koa-morgan": "^1.0.1",
    "koa-response-time": "^2.0.0",
    "koa-serve-static": "^1.0.0",
    "lodash": "^4.13.1",
    "minimist": "^1.2.0",
    "promise-toolbox": "^0.5.1",
    "spdy": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-babili": "0.0.5",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "dependency-check": "^2.5.1",
    "ghooks": "^1.2.4",
    "standard": "^8.1.0"
  },
  "scripts": {
    "build": "NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "depcheck": "dependency-check ./package.json --entry dist/worker-wrapper",
    "dev": "NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "lint": "standard",
    "posttest": "npm run lint && npm run depcheck",
    "prepublish": "npm run build"
  },
  "babel": {
    "env": {
      "production": {
        "presets": [
          "babili"
        ]
      }
    },
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "latest",
      "stage-0"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  },
  "config": {
    "ghooks": {
      "commit-msg": "npm test"
    }
  }
}
